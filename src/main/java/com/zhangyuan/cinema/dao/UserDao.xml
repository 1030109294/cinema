<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhangyuan.cinema.dao.UserDao">
    <select id="selectUserByOne" resultType="com.zhangyuan.cinema.domain.User">
select * from user where username = #{username} and password = #{password}
    </select>

    <select id="selectAllUser" resultType="com.zhangyuan.cinema.domain.User">
          select * from user
    </select>
</mapper>
